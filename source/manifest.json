{
  "name" : "rtcNoticer",
  "version" : "0.1",
  "description" : "RTC defect noticer",
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "contextMenus"
  ],
  "icons": { "16": "cookie.png", "48": "48.png","128": "128.png"},
  "options_page": "options.html",
  "background": { "scripts": ["background.js"] },
  "browser_action": {
    "default_icon": "cookie.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
    "web_accessible_resources": [
    "48.png"
  ]
}
